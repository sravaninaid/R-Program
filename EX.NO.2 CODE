# Define three 3x3 arrays
array1 <- matrix(c(1, 2, 3, 4, 5, 6, 7, 8, 9), nrow = 3, byrow = TRUE)
array2 <- matrix(c(10, 11, 12, 13, 14, 15, 16, 17, 18), nrow = 3, byrow = TRUE)
array3 <- matrix(c(19, 20, 21, 22, 23, 24, 25, 26, 27), nrow = 3, byrow = TRUE)

# Combine arrays row-wise
combined_array <- rbind(array1[1, ], array2[1, ], array3[1, ],
                        array1[2, ], array2[2, ], array3[2, ],
                        array1[3, ], array2[3, ], array3[3, ])

# Print the combined array
print("The combined array:")
print(combined_array)
